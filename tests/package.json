{
  "name": "@jupyterlab/benchmarks-tests",
  "version": "0.1.0",
  "description": "Benchmark tests",
  "repository": "https://github.com/jupyterlab/benchmarks",
  "author": "Jupyter Development Team",
  "license": "BSD-3-Clause",
  "private": true,
  "scripts": {
    "install:browser": "playwright install chromium",
    "start-jlab": "fkill -s :9999 && jupyter lab --config ./jupyter_server_config.py",
    "start-jlab:detached": "yarn run start-jlab&",
    "pretest": "tsc --incremental -p tsconfig.json",
    "test": "playwright test -c tests-out",
    "test:report": "http-server ./playwright-report -a localhost -o"
  },
  "devDependencies": {
    "@jupyterlab/galata": "^4.1.1",
    "@playwright/test": "^1.16.0",
    "@types/node-fetch": "^2.5.4",
    "fkill-cli": "6.0.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "4.1.5",
    "path": "~0.12.7",
    "typescript": "~4.1.3",
    "wait-on": "4.0.1"
  }
}
